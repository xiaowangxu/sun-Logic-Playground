config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Line",
"language": "GDScript",
"path": "res://Connection/Line.gd"
}, {
"base": "Node2D",
"class": "Module",
"language": "GDScript",
"path": "res://Module/ModuleScript/Module.gd"
}, {
"base": "Node2D",
"class": "Pin",
"language": "GDScript",
"path": "res://Pin/Pin.gd"
} ]
_global_script_class_icons={
"Line": "",
"Module": "",
"Pin": ""
}

[application]

config/name="sun Logic Playground"
run/main_scene="res://Playground.tscn"
config/icon="res://icon.png"

[autoload]

GlobalData="*res://GlobalData.gd"

[debug]

settings/fps/force_fps=120

[display]

window/size/width=800
window/dpi/allow_hidpi=true
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="expand"

